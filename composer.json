    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm-assets",
            "bower-asset-library": "vendor/bower-assets"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    },
    "repositories": [{
        "type": "composer",
        "url": "https://asset-packagist.org"
    }]
}
