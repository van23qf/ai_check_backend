        "cache/memcache-adapter": "^1.0",
        "cache/simple-cache-bridge": "^1.0",
        "dektrium/yii2-rbac": "^0.1.0",
        "dmstr/yii2-adminlte-asset": "2.*",
        "doctrine/cache": "^1.6",
